<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Songs</title>
</head>

<body>
  <div class="container">
    <%- include('layout/header') %>
    <div class= "jumbotron">
    <% songs.forEach(function(song){ %>
    <div class="row">
      <div class=" col-xs-12 text-center">
        <p> <%= song.title %> </p>
      </div>
    </div>
    <% }) %>
  </div>
</div>
</body>

</html>
